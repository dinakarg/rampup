        <tab2
            jcr:primaryType="cq:Widget"
            height="200"
            title="tab2"
            xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <title
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Title"
                    name="./jcr:title"
                    xtype="textfield"/>
                <url
                    jcr:primaryType="cq:Widget"
                    fieldLabel="URL"
                    name="./jcr:url"
                    xtype="textfield"/>
            </items>
        </tab2>
    </items>
